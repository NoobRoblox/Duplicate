--// GUI DUPE Progress by ChatGPT
--// Tương thích mọi executor (SAB, Fluxus, Synapse X, v.v.)

local TweenService = game:GetService("TweenService")

-- Xóa GUI cũ (nếu có)
if game.CoreGui:FindFirstChild("DupeUI") then
	game.CoreGui.DupeUI:Destroy()
end

-- Tạo GUI
local gui = Instance.new("ScreenGui")
gui.Name = "DupeUI"
gui.ResetOnSpawn = false
gui.Parent = game.CoreGui

-- Khung chính
local main = Instance.new("Frame")
main.Size = UDim2.new(0, 300, 0, 150)
main.Position = UDim2.new(0.5, -150, 0.5, -75)
main.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
main.BorderSizePixel = 2
main.BorderColor3 = Color3.fromRGB(170, 0, 255)
main.Parent = gui

-- Bo góc
local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0, 45)
corner.Parent = main

-- Nút DUPE
local button = Instance.new("TextButton")
button.Parent = main
button.Size = UDim2.new(0, 120, 0, 50)
button.Position = UDim2.new(0.5, -60, 0.5, -25)
button.BackgroundColor3 = Color3.fromRGB(60, 0, 120)
button.Text = "DUPE"
button.TextColor3 = Color3.fromRGB(255, 255, 255)
button.Font = Enum.Font.GothamBold
button.TextSize = 22
Instance.new("UICorner", button).CornerRadius = UDim.new(0, 12)

-- Thanh tiến trình
local progressBar = Instance.new("Frame")
progressBar.Parent = main
progressBar.Size = UDim2.new(0, 220, 0, 20)
progressBar.Position = UDim2.new(0.5, -110, 1, -40)
progressBar.BackgroundColor3 = Color3.fromRGB(30, 0, 60)
progressBar.Visible = false
Instance.new("UICorner", progressBar).CornerRadius = UDim.new(0, 10)

-- Phần đầy tiến trình
local fill = Instance.new("Frame")
fill.Parent = progressBar
fill.Size = UDim2.new(0, 0, 1, 0)
fill.BackgroundColor3 = Color3.fromRGB(180, 0, 255)
Instance.new("UICorner", fill).CornerRadius = UDim.new(0, 10)

-- Text hiển thị %
local percentLabel = Instance.new("TextLabel")
percentLabel.Parent = main
percentLabel.Size = UDim2.new(1, 0, 0, 30)
percentLabel.Position = UDim2.new(0, 0, 1, -70)
percentLabel.BackgroundTransparency = 1
percentLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
percentLabel.Font = Enum.Font.Gotham
percentLabel.TextSize = 18
percentLabel.Text = ""

-- Chức năng nút
button.MouseButton1Click:Connect(function()
	if progressBar.Visible then return end
	progressBar.Visible = true
	fill.Size = UDim2.new(0, 0, 1, 0)
	percentLabel.Text = "0%"
	
	local progress = 0
	local startTime = tick()

	while progress < 100 do
		task.wait(math.random(0.2, 0.6))
		local step = math.random(3, 8)
		progress = math.clamp(progress + step, 0, 100)

		TweenService:Create(fill, TweenInfo.new(0.3), {
			Size = UDim2.new(progress/100, 0, 1, 0)
		}):Play()

		percentLabel.Text = progress .. "%"
	end

	-- Giữ nguyên 15 giây sau khi hoàn thành
	task.wait(15)
	progressBar.Visible = false
	percentLabel.Text = ""
end)
