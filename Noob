--// Script GUI DUPE DELUXE by ChatGPT
--// T∆∞∆°ng th√≠ch SAB, Fluxus, Synapse X, Evon...
--// T√≠nh nƒÉng: Giao di·ªán bo g√≥c, DUPE progress, hi·ªáu ·ª©ng s√°ng t√≠m, √¢m thanh ho√†n t·∫•t, di chuy·ªÉn ng·∫´u nhi√™n

-- X√≥a GUI c≈©
if game.CoreGui:FindFirstChild("DupeUI") then
	game.CoreGui.DupeUI:Destroy()
end

local TweenService = game:GetService("TweenService")

-- GUI ch√≠nh
local gui = Instance.new("ScreenGui")
gui.Name = "DupeUI"
gui.ResetOnSpawn = false
gui.Parent = game.CoreGui

-- Khung ch√≠nh
local main = Instance.new("Frame")
main.Size = UDim2.new(0, 300, 0, 160)
main.Position = UDim2.new(0.5, -150, 0.5, -80)
main.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
main.BorderSizePixel = 0
main.Active = true
main.Draggable = true
main.Parent = gui

-- Hi·ªáu ·ª©ng ph√°t s√°ng vi·ªÅn t√≠m
local glow = Instance.new("ImageLabel")
glow.Parent = main
glow.Size = UDim2.new(1, 40, 1, 40)
glow.Position = UDim2.new(-0.07, 0, -0.07, 0)
glow.BackgroundTransparency = 1
glow.Image = "rbxassetid://5028857084" -- hi·ªáu ·ª©ng √°nh s√°ng m·ªù
glow.ImageColor3 = Color3.fromRGB(170, 0, 255)
glow.ImageTransparency = 0.3
glow.ZIndex = 0

-- Bo g√≥c
local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0, 30)
corner.Parent = main

-- Ti√™u ƒë·ªÅ
local title = Instance.new("TextLabel")
title.Parent = main
title.Size = UDim2.new(1, 0, 0, 40)
title.BackgroundTransparency = 1
title.Text = "NoobRoblox"
title.Font = Enum.Font.GothamBold
title.TextSize = 24
title.TextColor3 = Color3.fromRGB(255, 255, 255)
title.TextYAlignment = Enum.TextYAlignment.Center

-- N√∫t DUPE
local dupeBtn = Instance.new("TextButton")
dupeBtn.Parent = main
dupeBtn.Size = UDim2.new(0, 120, 0, 50)
dupeBtn.Position = UDim2.new(0.5, -60, 0.5, -10)
dupeBtn.BackgroundColor3 = Color3.fromRGB(60, 0, 120)
dupeBtn.Text = "DUPE"
dupeBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
dupeBtn.Font = Enum.Font.GothamBold
dupeBtn.TextSize = 22
Instance.new("UICorner", dupeBtn).CornerRadius = UDim.new(0, 12)

-- Thanh ti·∫øn tr√¨nh
local progressBar = Instance.new("Frame")
progressBar.Parent = main
progressBar.Size = UDim2.new(0, 220, 0, 20)
progressBar.Position = UDim2.new(0.5, -110, 1, -35)
progressBar.BackgroundColor3 = Color3.fromRGB(30, 0, 60)
progressBar.Visible = false
Instance.new("UICorner", progressBar).CornerRadius = UDim.new(0, 10)

local fill = Instance.new("Frame")
fill.Parent = progressBar
fill.Size = UDim2.new(0, 0, 1, 0)
fill.BackgroundColor3 = Color3.fromRGB(180, 0, 255)
Instance.new("UICorner", fill).CornerRadius = UDim.new(0, 10)

local percentLabel = Instance.new("TextLabel")
percentLabel.Parent = main
percentLabel.Size = UDim2.new(1, 0, 0, 25)
percentLabel.Position = UDim2.new(0, 0, 1, -60)
percentLabel.BackgroundTransparency = 1
percentLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
percentLabel.Font = Enum.Font.Gotham
percentLabel.TextSize = 18
percentLabel.Text = ""

-- N√∫t tr√≤n c√≥ th·ªÉ g·∫Øn ·∫£nh c·ªßa b·∫°n
local circleBtn = Instance.new("ImageButton")
circleBtn.Parent = main
circleBtn.Size = UDim2.new(0, 35, 0, 35)
circleBtn.Position = UDim2.new(1, -45, 0, 10)
circleBtn.BackgroundColor3 = Color3.fromRGB(40, 0, 80)
circleBtn.Image = "rbxassetid://137566338679153" -- üî∏ Thay YOUR_IMAGE_ID_HERE b·∫±ng ID ·∫£nh Roblox c·ªßa b·∫°n
Instance.new("UICorner", circleBtn).CornerRadius = UDim.new(1, 0)

-- Hi·ªáu ·ª©ng s√°ng xoay nh·∫π quanh glow
task.spawn(function()
	while task.wait(0.05) do
		glow.Rotation = glow.Rotation + 0.5
	end
end)

-- √Çm thanh ‚Äúding‚Äù khi ho√†n t·∫•t
local dingSound = Instance.new("Sound")
dingSound.Parent = main
dingSound.SoundId = "rbxassetid://9118823102" -- √¢m thanh nh·∫π "ding"
dingSound.Volume = 1

-- N√∫t ƒë√≥ng/m·ªü GUI
local minimized = false
circleBtn.MouseButton1Click:Connect(function()
	minimized = not minimized
	if minimized then
		TweenService:Create(main, TweenInfo.new(0.4, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {Size = UDim2.new(0, 40, 0, 40)}):Play()
		for _, v in pairs(main:GetChildren()) do
			if v ~= circleBtn and v ~= corner and v ~= glow then
				v.Visible = false
			end
		end
	else
		TweenService:Create(main, TweenInfo.new(0.4, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {Size = UDim2.new(0, 300, 0, 160)}):Play()
		task.wait(0.4)
		for _, v in pairs(main:GetChildren()) do
			if v ~= circleBtn and v ~= corner and v ~= glow then
				v.Visible = true
			end
		end
	end
end)

-- N√∫t DUPE ho·∫°t ƒë·ªông
dupeBtn.MouseButton1Click:Connect(function()
	if progressBar.Visible then return end
	progressBar.Visible = true
	fill.Size = UDim2.new(0, 0, 1, 0)
	percentLabel.Text = "0%"

	local progress = 0
	while progress < 100 do
		task.wait(math.random(0.2, 0.6))
		local step = math.random(3, 8)
		progress = math.clamp(progress + step, 0, 100)
		TweenService:Create(fill, TweenInfo.new(0.3), {Size = UDim2.new(progress/100, 0, 1, 0)}):Play()
		percentLabel.Text = progress .. "%"
	end

	dingSound:Play() -- ph√°t √¢m thanh khi ho√†n t·∫•t
	task.wait(15)
	progressBar.Visible = false
	percentLabel.Text = ""
end)

-- Giao di·ªán di chuy·ªÉn ng·∫´u nhi√™n
task.spawn(function()
	while task.wait(3) do
		if not minimized then
			local randomX = math.random(50, 600)
			local randomY = math.random(50, 300)
			TweenService:Create(main, TweenInfo.new(2, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {
				Position = UDim2.new(0, randomX, 0, randomY)
			}):Play()
		end
	end
end)
